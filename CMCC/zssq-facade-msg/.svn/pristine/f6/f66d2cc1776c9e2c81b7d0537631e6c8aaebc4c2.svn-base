package com.zssq.model;

import java.io.Serializable;
import java.util.List;

import com.alibaba.fastjson.JSONObject;

/**
 * 
 * @ClassName: MessageBordAndReplyList  
 * @Description: 留言
 * @author YDB  
 * @date 2017年3月27日  
 *
 */
public class MessageBordAndReplyModel implements Serializable{

	//留言人名称
	private String messageBoardUsername; 	
	//留言内容
	private String content;
	//留言code
	private String msgCode;
	//留言时间
	private Long creatTime;
	//用户名称
	private String userName;
	//回复list
	private List<JSONObject> replyList;
	//回复总条数
	private int replyCount;
	//用户code
	private String userCode;
	//留言用户code
	private String boardUserCode;
	
	
	
	public String getBoardUserCode() {
		return boardUserCode;
	}
	public void setBoardUserCode(String boardUserCode) {
		this.boardUserCode = boardUserCode;
	}
	public String getUserCode() {
		return userCode;
	}
	public void setUserCode(String userCode) {
		this.userCode = userCode;
	}
	public int getReplyCount() {
		return replyCount;
	}
	public void setReplyCount(int replyCount) {
		this.replyCount = replyCount;
	}
	public String getMsgCode() {
		return msgCode;
	}
	public void setMsgCode(String msgCode) {
		this.msgCode = msgCode;
	}
	public String getUserName() {
		return userName;
	}
	public void setUserName(String userName) {
		this.userName = userName;
	}
	public String getMessageBoardUsername() {
		return messageBoardUsername;
	}
	public void setMessageBoardUsername(String messageBoardUsername) {
		this.messageBoardUsername = messageBoardUsername;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}

	public Long getCreatTime() {
		return creatTime;
	}
	public void setCreatTime(Long creatTime) {
		this.creatTime = creatTime;
	}
	public List<JSONObject> getReplyList() {
		return replyList;
	}
	public void setReplyList(List<JSONObject> replyList) {
		this.replyList = replyList;
	}
	
	
	
}
