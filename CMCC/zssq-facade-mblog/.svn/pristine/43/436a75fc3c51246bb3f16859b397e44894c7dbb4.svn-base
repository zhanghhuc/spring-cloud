    /** 代发人CODE **/
    private String agentUserCode = "";
    /** 代发人组织CODE **/
    private String agentOrgCode = "";
    /** 班组CODE **/
    private String teamCode = "";
    /** 来源微博CODE **/
    private String sourceMblogCode = "";
    /** 来源微博发布人CODE **/
    private String sourceUserCode = "";
    /** 微博所属 **/
    private Byte mblogDepend = 1;
    /** 微博来源 **/
    private Byte mblogSource = 1;
