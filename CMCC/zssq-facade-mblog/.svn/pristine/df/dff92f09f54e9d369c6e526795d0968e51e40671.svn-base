    public void setTopicNum(Integer topicNum) {
        this.topicNum = topicNum;
    }

    public String getTenantCode() {
        return tenantCode;
    }

    public void setTenantCode(String tenantCode) {
        this.tenantCode = tenantCode == null ? "" : tenantCode.trim();
    }
}