		AND collector_code = #{collectorCode}
		AND tenant_code = #{tenantCode}
  </delete>
  
  <!-- 查询我的收藏列表 -->
  <select id="selectCollectionList" parameterType="java.util.Map" resultType="com.zssq.dao.pojo.VoteInfo">
	  SELECT 
		  vc.id as id,
		  vi.code as code,
		  vi.sponsor_code as sponsorCode,
		  vi.sponsor_type as sponsorType,
		  vi.sponsor_org_code as sponsorOrgCode,
		  vi.vote_status as voteStatus,
		  vi.title as title,
		  vi.start_time as startTime,
		  vi.end_time as endTime,
		  vi.create_time as createTime,
		  vi.collection_num as collectionNum,
		  vi.share_num as shareNum,
		  vi.comment_num as commentNum,
		  vi.praise_num as praiseNum,
		  vi.join_num as joinNum,
		  vc.create_time AS collectorTime,
		  vp.code AS pCode
		FROM
		  vote_collection vc 
		JOIN  vote_info vi
		    ON vi.code = vc.vote_info_code 
		    AND vc.is_disable = 0 
		    AND vc.is_delete = 0 
		  LEFT JOIN vote_praise vp 
		    ON vi.code = vp.info_code 
		    AND vp.info_type = 1 
		    AND vp.is_disable = 0 
		    AND vp.is_delete = 0 
		    AND vp.tenant_code = #{tenantCode} 
		    AND vp.admirer_code = #{userCode} 
		WHERE vi.is_disable = 0 
		  AND vi.is_delete = 0 
		  AND vc.collector_code = #{userCode}
		  <if test="id != null and id>0" >
			AND vc.id <![CDATA[<]]> #{id} 
		  </if>
		ORDER BY vc.id DESC 
		LIMIT #{pageSize}
  </select>
  
  
