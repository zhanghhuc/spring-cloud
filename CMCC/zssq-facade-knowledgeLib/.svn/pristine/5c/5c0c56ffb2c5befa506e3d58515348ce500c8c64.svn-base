package com.zssq.vo;

import java.io.Serializable;
/**
 * 
 * @ClassName: RepositoryPraiseVo  
 * @Description: 知识库点赞  
 * @author sry  
 * @date 2017年4月28日  
 *
 */
public class RepositoryPraiseVo implements Serializable{

	private static final long serialVersionUID = 2239734945030975593L;
	
	private String objectCode;//被点赞code（知识code或追加知识codde）
	
	private Byte praiseType;//操作类型：1.知识；2.追加知识
	
	private Byte actionClass;//操作类型：1.点赞；2.取消点赞
	
	private String userCode;//用户编号

	public String getObjectCode() {
		return objectCode;
	}

	public void setObjectCode(String objectCode) {
		this.objectCode = objectCode;
	}

	
	public Byte getPraiseType() {
		return praiseType;
	}

	public void setPraiseType(Byte praiseType) {
		this.praiseType = praiseType;
	}

	public Byte getActionClass() {
		return actionClass;
	}

	public void setActionClass(Byte actionClass) {
		this.actionClass = actionClass;
	}

	public String getUserCode() {
		return userCode;
	}

	public void setUserCode(String userCode) {
		this.userCode = userCode;
	}
	
	

}
