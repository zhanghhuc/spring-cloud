package com.zssq.credit.pojo;

import java.util.Date;

/**
 * 
 * @ClassName: CreditAccountUser  
 * @Description: 班组积分、金币账户实体  
 * @author CaiZhaohui  
 * @date 2017年6月20日  
 *
 */
public class CreditAccountTeam {

    private Integer teamId;
    
    private String teamCode;
    
    private String orgCode;

    private Double pointNum;

    private Double goldCoin;
    
    private Date createTime;

    private Date updateTime;

	public Integer getTeamId() {
		return teamId;
	}

	public void setTeamId(Integer teamId) {
		this.teamId = teamId;
	}

	public String getTeamCode() {
		return teamCode;
	}

	public void setTeamCode(String teamCode) {
		this.teamCode = teamCode;
	}

	public String getOrgCode() {
		return orgCode;
	}

	public void setOrgCode(String orgCode) {
		this.orgCode = orgCode;
	}

	public Double getPointNum() {
		return pointNum;
	}

	public void setPointNum(Double pointNum) {
		this.pointNum = pointNum;
	}

	public Double getGoldCoin() {
		return goldCoin;
	}

	public void setGoldCoin(Double goldCoin) {
		this.goldCoin = goldCoin;
	}

	public Date getCreateTime() {
		return createTime;
	}

	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}

	public Date getUpdateTime() {
		return updateTime;
	}

	public void setUpdateTime(Date updateTime) {
		this.updateTime = updateTime;
	}

}