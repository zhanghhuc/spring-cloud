/*地图开始*/
.groupBox{width: 100%;}
.Group-Box{overflow: hidden;min-width:1100px;width:auto !important;width:1100px;}
.group-box{width: 1100px;margin:0 auto;padding:0 20px;background: #fff;}
.group-intent1 {width: 1100px;margin:0 auto ;padding: 22px 20px 35px 20px;background: #fff;}
.group-intent2{min-height:2000px;height:auto !important;height:2000px;}
.group-intent1 .lr{height: 40px;background: #f6f6f6;}
.group-intent1 .intent1-left{float: left;height: 40px;width: 826px;}
.group-intent1 .intent1-right{float: right;}

.group-intent1 .intent1-left li{float: left;width: 90px;height: 40px;background: #f6f6f6; }
.group-intent1 .intent1-left li a{float: left;width: 89px;height: 26px;line-height: 26px;border-right: 1px solid #dddddd;text-align: center;font-size: 18px;color: #000;margin-top:7px;}
/*如果li有class="tab-bg",则a的border-right=0*/
.group-intent1 .intent1-left li.last a,.group-intent1 .intent1-left li.tab-bg a{border-right: 0 none;}

/*此处是点击出现地图的按钮*/
.group-intent1 .intent1-right{width: 274px;height: 36px;padding-top: 4px;}
.group-intent1 .intent1-right .china-mobile{float: right;height:26px;line-height: 26px;font-size: 14px;padding-left: 16px;margin-right: 6px;cursor: pointer;width: 126px;}
.group-intent1 .intent1-right .change-city{float: right;position: relative;width: 74px;height:26px;border: 1px solid #dddada;line-height: 26px;padding-right: 3px;margin-right: 6px;}
.group-intent1 .intent1-right .change-city select{width: 74px;height:26px;line-height: 26px;padding-left: 6px; font-size: 14px;color: #999999;border:0 none;background: url(../images/arrow-down.png) no-repeat right center;cursor: pointer;}
.group-intent1 .intent1-right .change-city .arrow-border{width: 1px;height: 26px;background:#dddada ;position: absolute;right: 20px; top:0;}
.group-intent1 .intent1-map{display: block;height: 477px;margin-top: 7px;}
/*此处为地图及地图上的内容 控制他的高度来收起*/
.intent1-map {position: relative;display: block;/*height: 462px;*/}
/*此处为地图的样式*/
.intent1-map .map{float: left;}
/*此处为地图上列表的样式*/
.intent1-map .map-list{float: right;width: 310px;margin: 46px 68px 0 0;}
.intent1-map .map-list .list-head{width: 280px;height:30px;line-height: 30px;border:1px solid #d0e6e6;padding: 0 14px;}
.intent1-map .map-list .list-head .map-title{font-size: 16px;color: #333333;float: left;}
.intent1-map .map-list .list-head .map-data1{color:#666666;font-size: 14px;float: right;}
.intent1-map .list-intent{height: 330px;padding: 0 14px 0 20px;overflow-y:auto; }
.intent1-map .list-intent li.list-common{height: 82px;border-bottom: 1px solid #dbe8ea;}
.intent1-map .list-intent .touxiang{float: left;width:60px ;height:60px ;border-radius: 50%;margin-top: 8px;}
.intent1-map .list-intent .touxiang img{width: 100%;height: 100%;border-radius: 50%;}
.intent1-map .list-intent .explain-head{float: left;width: 182px;padding-left: 10px;margin-top: 6px;}
.intent1-map .list-intent .explain-head .beijing{font-size: 16px;color: #333333;}
.intent1-map .list-intent .explain-head .first-data{color: #666666;font-size: 12px;padding:2px 0 2px 18px;background: url(../images/ji.png) no-repeat left center;margin-left: 6px;}
.intent1-map .list-intent .on-sale{color: #666666;font-size: 14px;}
.intent1-map .list-intent .people-count,.intent1-map .list-intent .recommend-people{font-size: 12px;color: #666666;}
.intent1-map .list-intent .recommend-people,.group-intent2 .intent2-left .news-list li a,.left-list2 .sb-trends .sb-explain .news-details1 a.ovf{ text-overflow:ellipsis; white-space:nowrap; overflow:hidden;    width: 570px;display: inline-block;}
.intent1-map .list-intent .explain-head li.last1{border-bottom: none 0;}
.intent1-map .map-count{position: absolute;bottom: 28px;left: 30px;}
.intent1-map .map-count span{display: block;font-size: 14px; color: #fff;}
.intent1-map .map-btn{padding-left:26px;position: absolute;bottom: 10px;right:12px;font-size: 14px;cursor: pointer;}
.group-intent2 .intent2-left{float: left;width: 710px;}
.group-intent2 .intent2-right{float: right;width: 352px;}

